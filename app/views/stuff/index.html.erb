<div class="header">
  <div class="container">
    <h1>J-Rod</h1>
  </div>
</div>

<div class="tags">
  <div class="container">
    <div class="cards row">
    	<% @stuff.each do |s| %>
        <div class="row">
          <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
              <%= image_tag s.image %>
              <div class="caption">
                <h3><%= s.name %></h3>
                <p><%= s.description %></p>
                <p><a href="/edit" class="btn btn-primary" role="button">Edit</a> <a href="/delete" class="btn btn-default" role="button">Delete</a></p>
              </div>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
